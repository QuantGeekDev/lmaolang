{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["xo", "prettier"],
  "overrides": [
    {
      "extends": ["xo-typescript", "prettier"],
      "files": ["*.ts", "*.tsx"],
      "rules": {
        "@typescript-eslint/naming-convention": "off",
        "@typescript-eslint/consistent-type-definitions": "interface"
      }
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {}
}
